import std.hash;

func hash_data(data: []const u8) -> u64 {.effects: "pure"} {
    return std.hash.fnv1a(data);
}

func process_batch(items: [][]const u8) -> []u64 {
    var results = make([]u64, items.len);
    for (i, item) in enumerate(items) {
        results[i] = hash_data(item);
    }
    return results;
}
