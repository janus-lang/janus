// Dependent module - should be invalidated when a.jan changes

import a;

func dependent_function(z: i32) -> i32 {
    return a.base_function(z) + a.utility_function(z);
}

func independent_function(w: i32) -> i32 {
    return w * w; // Does not depend on module a
}
