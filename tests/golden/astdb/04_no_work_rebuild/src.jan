import std.io;

func main() -> void {
    std.io.println("Hello, Janus!");

    let result = compute(42);
    std.io.println("Result: {}", result);
}

func compute(n: i32) -> i32 {
    if (n <= 0) return 0;
    return n * n + compute(n - 1);
}
