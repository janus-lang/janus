func process(x: i32) -> i32 {
    x * 2 + 1
}

func process(x: f64) -> f64 {
    x * 2.0 + 1.0
}

func process(x: string) -> string {
    x ++ "_processed"
}

func process(x: bool) -> string {
    if x { "true_processed" } else { "false_processed" }
}

func main() {
    let result1 = process(42)
    let result2 = process(3.14)
    let result3 = process("test")
    let result4 = process(true)
}
