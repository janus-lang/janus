// Standard Collections Module
// This module provides Vector, HashMap, and other essential data structures.

pub struct Vector {
    capacity: u64,
    len: u64,
    data: ?*void
}

pub fn new_vector() -> Vector {
    return Vector {
        capacity: 0,
        len: 0,
        data: null
    }
}
