struct Vector {
    capacity: u64,
    len: u64,
    data: ?*void
}

test "vector_struct_literal" do
    let v = Vector {
        capacity: 10,
        len: 0,
        data: null
    }
    assert(v.len == 0)
    assert(v.capacity == 10)
end
