// :service profile - async/await example (Phase 1)
// This tests that the parser recognizes async function syntax

async func greet(name: []const u8) !void do
    println("Hello, ", name, "!")
end

async func main() !void do
    await greet("World")
    await greet("Janus")
    println("Done!")
end
